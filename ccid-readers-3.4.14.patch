@@ -0,0 +1,32 @@
+#!/usr/bin/perl
+
+sub get_index {
+    my @index = split(':',$_[0]);
+    return $index[0].":".$index[1];
+}
+
+my %hash_array;
+sub read_reader_list {
+    local $FH;
+    open FH, "< @_" or die "Can't open '@_': $!";
+
+    %hash_array = ();
+    THISWHILE: while (<FH>) {
+	next THISWHILE if /^#/;
+	next THISWHILE if /^$/;
+	$hash_array{get_index($_)} =$_;
+    }
+    close FH;
+    return %hash_array;
+}
+
+my $newreaderlist = $ARGV[0];
+my $oldreaderlist = $ARGV[1];
+
+my %old_hash = read_reader_list($oldreaderlist);
+my %new_hash = read_reader_list($newreaderlist);
+
+foreach ( keys %new_hash) {
+    print $new_hash{$_} if !defined($old_hash{$_});
+}
+
